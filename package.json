{
  "name": "air-quality-backend",
  "version": "1.0.0",
  "description": "Air Quality Backend API with Clean Architecture",
  "main": "dist/app.js",
"scripts": {
    "dev": "nodemon src/app.ts",
    "dev:worker": "nodemon src/workers/ingestion.worker.ts",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:worker\"",
    "build": "tsc",
    "start": "node dist/app.js",
    "start:worker": "node dist/workers/ingestion.worker.js",
    "start:all": "concurrently \"npm run start\" \"npm run start:worker\"",
    "lint": "eslint src --ext .ts",
    "test": "jest"
  },
  "keywords": [
    "air-quality",
    "api",
    "typescript",
    "express",
    "clean-architecture"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "csv-parse": "^6.1.0",
    "csv-parser": "^3.0.0",
    "dotenv": "^16.3.0",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.0",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.11.0",
    "pg-hstore": "^2.3.4",
    "redis": "^4.6.0",
    "sequelize": "^6.35.0",
    "typescript": "^5.0.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/compression": "^1.7.4",
    "@types/cors": "^2.8.15",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.6",
    "@types/multer": "^1.4.8",
    "@types/node": "^20.8.0",
    "@types/pg": "^8.10.0",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "concurrently": "^9.2.0",
    "eslint": "^8.51.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0"
  }
}
